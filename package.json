{
  "name": "typescript-react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "lint": "npm run lint-ts && npm run lint-css",
    "lint-css": "stylelint './src/**/*.css'",
    "lint-ts": "tslint 'src/**/*.tsx' 'src/**/*.ts' 'test/**/*.tsx' 'test/**/*.ts'",
    "postinstall": "npm run typings && npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "pretest": "npm run lint",
    "test": "ts-node ./node_modules/.bin/_mocha --opts ./test-utils/mocha.opts",
    "test:watch": "ts-node ./node_modules/.bin/_mocha --opts ./test-utils/mocha.opts -w",
    "typings": "rimraf typings/ && typings install"
  },
  "keywords": [],
  "dependencies": {
    "express": "^4.13.4",
    "helmet": "^1.3.0",
    "http-proxy": "^1.13.2",
    "winston": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "chai": "^3.5.0",
    "classnames": "^2.2.3",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "cssnano": "^3.5.2",
    "enzyme": "^2.2.0",
    "es5-shim": "^4.5.6",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "history": "^2.0.1",
    "html-webpack-plugin": "^2.9.0",
    "immutable": "^3.7.6",
    "immuts": "0.1.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "node-sass": "^3.10.0",
    "object-assign": "^4.0.1",
    "office-ui-fabric-react": "^0.48.0",
    "raw-loader": "^0.5.1",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.2.3",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "require-hacker": "^2.1.3",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "stylelint": "^6.2.0",
    "stylelint-webpack-plugin": "^0.2.0",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.7.2",
    "tslint": "^3.7.4",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.9.1"
  }
}
